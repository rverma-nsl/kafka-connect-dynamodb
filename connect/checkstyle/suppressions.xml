<?xml version="1.0" ?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.1//EN"
        "https://checkstyle.org/dtds/suppressions_1_1.dtd">

<suppressions>
    <suppress checks="Javadoc" files=".+\.java" />
    <suppress checks="HiddenField" files=".+\.java" />
    <suppress checks="LineLength" files=".+\.java" />
    <suppress checks="DesignForExtension" files=".+\.java" />
    <suppress checks="AvoidStarImport" files=".+\.java" />
    <suppress checks="FinalParameters" files=".+\.java" />
    <suppress checks="VisibilityModifier" files=".+\.java" />
    <suppress checks="NoWhitespaceBefore" files=".+\.java" />
    <suppress checks="OperatorWrap" files=".+\.java" />
    <suppress checks="MagicNumber" files=".+\.java" />
    <suppress checks="AvoidNestedBlocks" files=".+\.java" />
    <suppress checks="HideUtilityClassConstructor" files=".+\Version.java" />
</suppressions>